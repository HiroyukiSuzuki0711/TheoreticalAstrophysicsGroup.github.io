@ARTICLE{2006Sci...312.1334A,
   author = {{Abe}, M. and {Takagi}, Y. and {Kitazato}, K. and {Abe}, S. and 
	{Hiroi}, T. and {Vilas}, F. and {Clark}, B.~E. and {Abell}, P.~A. and 
	{Lederer}, S.~M. and {Jarvis}, K.~S. and {Nimura}, T. and {Ueda}, Y. and 
	{Fujiwara}, A.},
    title = "{Near-Infrared Spectral Results of Asteroid Itokawa from the Hayabusa Spacecraft}",
  journal = {Science},
     year = 2006,
    month = jun,
   volume = 312,
    pages = {1334-1338},
 abstract = "{The near-infrared spectrometer on board the Japanese Hayabusa spacecraft
found a variation of more than 10\% in albedo and absorption band depth
in the surface reflectance of asteroid 25143 Itokawa. Spectral shape
over the 1-micrometer absorption band indicates that the surface of this
body has an olivine-rich mineral assemblage potentially similar to that
of LL5 or LL6 chondrites. Diversity in the physical condition of
Itokawa's surface appears to be larger than for other S-type asteroids
previously explored by spacecraft, such as 433 Eros.
}",
      doi = {10.1126/science.1125718},
   adsurl = {http://adsabs.harvard.edu/abs/2006Sci...312.1334A},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2006Sci...312.1330F,
   author = {{Fujiwara}, A. and {Kawaguchi}, J. and {Yeomans}, D.~K. and 
	{Abe}, M. and {Mukai}, T. and {Okada}, T. and {Saito}, J. and 
	{Yano}, H. and {Yoshikawa}, M. and {Scheeres}, D.~J. and {Barnouin-Jha}, O. and 
	{Cheng}, A.~F. and {Demura}, H. and {Gaskell}, R.~W. and {Hirata}, N. and 
	{Ikeda}, H. and {Kominato}, T. and {Miyamoto}, H. and {Nakamura}, A.~M. and 
	{Nakamura}, R. and {Sasaki}, S. and {Uesugi}, K.},
    title = "{The Rubble-Pile Asteroid Itokawa as Observed by Hayabusa}",
  journal = {Science},
     year = 2006,
    month = jun,
   volume = 312,
    pages = {1330-1334},
 abstract = "{During the interval from September through early December 2005, the
Hayabusa spacecraft was in close proximity to near-Earth asteroid 25143
Itokawa, and a variety of data were taken on its shape, mass, and
surface topography as well as its mineralogic and elemental abundances.
The asteroid's orthogonal axes are 535, 294, and 209 meters, the mass is
3.43 {\times} 10$^{10}$ kilograms, and the estimated bulk density
is 1.9 +/- 0.13 grams per cubic centimeter. The correspondence between
the smooth areas on the surface (Muses Sea and Sagamihara) and the
gravitationally low regions suggests mass movement and an effective
resurfacing process by impact jolting. Itokawa is considered to be a
rubble-pile body because of its low bulk density, high porosity,
boulder-rich appearance, and shape. The existence of very large boulders
and pillars suggests an early collisional breakup of a preexisting
parent asteroid followed by a re-agglomeration into a rubble-pile
object.
}",
      doi = {10.1126/science.1125841},
   adsurl = {http://adsabs.harvard.edu/abs/2006Sci...312.1330F},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2005A&A...443..347M,
   author = {{M{\"u}ller}, T.~G. and {Sekiguchi}, T. and {Kaasalainen}, M. and 
	{Abe}, M. and {Hasegawa}, S.},
    title = "{Thermal infrared observations of the Hayabusa spacecraft target asteroid <ASTROBJ>25143 Itokawa</ASTROBJ>}",
  journal = {\aap},
   eprint = {astro-ph/0509434},
 keywords = {minor planets, asteroids, radiation mechanisms: thermal, infrared: solar system},
     year = 2005,
    month = nov,
   volume = 443,
    pages = {347-355},
 abstract = "{We obtained N- and Q-band observations of the Apollo-type asteroid
<ASTROBJ>25143 Itokawa</ASTROBJ> during its close Earth approach in July
2004 with TIMMI2 at the ESO 3.6 m telescope. Our photometric
measurement, in combination with already published data, allowed us to
derive a radiometric effective diameter of 0.32 {\plusmn} 0.03 km and an
albedo of 0.19$^{+0.11}$$_{-0.03}$ through a thermophysical
model. This effective diameter corresponds to a slightly asymmetrical
and flattened ellipsoid of the approximate size of 520({\plusmn}50)
{\times} 270({\plusmn}30) {\times} 230({\plusmn}20) m, based on the
Kaasalainen et al. (2005, Proceedings of the 1st Hayabusa Symposium, ASP
Conf. Ser., submitted) shape model. Our studies show that the thermal
observations lead to size estimates which are about 15\% smaller than the
radar results (Ostro et al. 2005, Met. Plan. Sci., submitted), slightly
outside the stated radar uncertainties of {\plusmn}10\%. We determined a
rather high thermal inertia of 750 J m$^{-2}$ s$^{-0.5}$
K$^{-1}$. This is an indication for a bare rock dominated surface,
a thick dust regolith can be excluded as well as a metallic surface.
From our data we constructed a 10.0 {$\mu$}m thermal lightcurve which is
nicely matched in amplitude and phase by the shape and spin vector
solution in combination with our TPM description. The assumed S-type
bulk density in combination with radiometric size lead to a total mass
estimate of 4.5$^{+2.0}$$_{-1.8}$ {\times} 10$^{10}$
kg.
}",
      doi = {10.1051/0004-6361:20053862},
   adsurl = {http://adsabs.harvard.edu/abs/2005A%26A...443..347M},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2005Icar..173..153L,
   author = {{Lederer}, S.~M. and {Domingue}, D.~L. and {Vilas}, F. and {Abe}, M. and 
	{Farnham}, T.~L. and {Jarvis}, K.~S. and {Lowry}, S.~C. and 
	{Ohba}, Y. and {Weissman}, P.~R. and {French}, L.~M. and {Fukai}, H. and 
	{Hasegawa}, S. and {Ishiguro}, M. and {Larson}, S.~M. and {Takagi}, Y.
	},
    title = "{Physical characteristics of Hayabusa target Asteroid 25143 Itokawa}",
  journal = {\icarus},
     year = 2005,
    month = jan,
   volume = 173,
    pages = {153-165},
 abstract = "{In March 2001, the Hayabusa spacecraft target, Asteroid 25143 Itokawa,
made its final close approach to Earth prior to the spacecraft's launch.
We carried out an extensive observing campaign from January to September
2001 to better characterize this near-Earth asteroid. Global physical
properties of the surface of Itokawa were characterized by analyzing its
photometric properties and behavior. Results included here capitalize on
analysis of broadband photometric observations taken with a number of
telescopes, instruments, and observers. We employed a Hapke model to
estimate the surface roughness, single particle scattering albedo,
single particle scattering characteristics, phase integral, and
geometric and bond albedo. We find that this asteroid has a higher
geometric albedo than average main belt S-class asteroids; this is
consistent with results from other observers. The broadband colors of
Itokawa further support evidence that this is an atypical S-class
asteroid. Broadband colors show spectral characteristics more typically
found on large-diameter main-belt asteroids believed to be
space-weathered, suggesting the surface of this small diameter,
near-Earth asteroid could likewise be space-weathered.
}",
      doi = {10.1016/j.icarus.2004.07.022},
   adsurl = {http://adsabs.harvard.edu/abs/2005Icar..173..153L},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2004AdSpR..34.2281M,
   author = {{Morimoto}, M. and {Yamakawa}, H. and {Yoshikawa}, M. and {Abe}, M. and 
	{Yano}, H.},
    title = "{Trajectory design of multiple asteroid sample return missions}",
  journal = {Advances in Space Research},
     year = 2004,
    month = jan,
   volume = 34,
    pages = {2281-2285},
 abstract = "{As one of the potential candidates of the ''post-MUSES-C'' minor body
exploration to be launched around 2010, we have investigated scientific
justifications and feasible mission scenarios. Two mission types are
focused on among many other proposed mission concepts. One is the
multiple rendezvous and sample return mission to asteroids whose
spectral type is already known. The other is the multiple fly-bys and
sample return mission to an asteroid family. This paper reports the
preliminary design results of these two proposed missions.
}",
      doi = {10.1016/j.asr.2003.10.055},
   adsurl = {http://adsabs.harvard.edu/abs/2004AdSpR..34.2281M},
  adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}

@ARTICLE{2003A&A...405L..29K,
   author = {{Kaasalainen}, M. and {Kwiatkowski}, T. and {Abe}, M. and {Piironen}, J. and 
	{Nakamura}, T. and {Ohba}, Y. and {Dermawan}, B. and {Farnham}, T. and 
	{Colas}, F. and {Lowry}, S. and {Weissman}, P. and {Whiteley}, R.~J. and 
